module Dodopayments
  module Models
    type product =
      {
        business_id: String,
        created_at: Time,
        is_recurring: bool,
        license_key_enabled: bool,
        price: Dodopayments::Models::price,
        product_id: String,
        tax_category: Dodopayments::Models::tax_category,
        updated_at: Time,
        addons: ::Array[String]?,
