        brand_id: String,
        business_id: String,
        created_at: Time,
        credit_entitlements: ::Array[Dodopayments::Product::CreditEntitlement],
        is_recurring: bool,
        license_key_enabled: bool,
        metadata: ::Hash[Symbol, String],
        price: Dodopayments::Models::price,
        product_id: String,
        tax_category: Dodopayments::Models::tax_category,
        updated_at: Time,
        ?addons: ::Array[String]?,
        ?description: String?,
        ?digital_product_delivery: Dodopayments::Product::DigitalProductDelivery?,
        ?image: String?,
        ?license_key_activation_message: String?,
        ?license_key_activations_limit: Integer?,
        ?license_key_duration: Dodopayments::LicenseKeyDuration?,
        ?name: String?,
        ?product_collection_id: String?
      ) -> void

      def to_hash: -> {
        brand_id: String,
        business_id: String,
        created_at: Time,
        credit_entitlements: ::Array[Dodopayments::Product::CreditEntitlement],
        is_recurring: bool,
        license_key_enabled: bool,
        metadata: ::Hash[Symbol, String],
        price: Dodopayments::Models::price,
        product_id: String,
        tax_category: Dodopayments::Models::tax_category,
        updated_at: Time,
        addons: ::Array[String]?,
        description: String?,
        digital_product_delivery: Dodopayments::Product::DigitalProductDelivery?,
