        license_key_activations_limit: Integer?,
        license_key_duration: Dodopayments::LicenseKeyDuration?,
        name: String?,
        product_collection_id: String?
      }

      type credit_entitlement =
        {
          id: String,
          credit_entitlement_id: String,
          credit_entitlement_name: String,
          credit_entitlement_unit: String,
          credits_amount: String,
          credits_reduce_overage: bool,
          overage_charge_at_billing: bool,
          overage_enabled: bool,
          preserve_overage_at_reset: bool,
          proration_behavior: Dodopayments::Models::Product::CreditEntitlement::proration_behavior,
          rollover_enabled: bool,
          trial_credits_expire_after_trial: bool,
          currency: Dodopayments::Models::currency?,
          expires_after_days: Integer?,
          low_balance_threshold_percent: Integer?,
          max_rollover_count: Integer?,
          overage_limit: String?,
