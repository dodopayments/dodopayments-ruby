module Dodopayments
  module Models
    type product_list_params =
      {
        archived: bool,
        brand_id: String?,
        page_number: Integer?,
        page_size: Integer?,
        recurring: bool?
      }
      & Dodopayments::Internal::Type::request_parameters

    class ProductListParams < Dodopayments::Internal::Type::BaseModel
      extend Dodopayments::Internal::Type::RequestParameters::Converter
      include Dodopayments::Internal::Type::RequestParameters

      attr_reader archived: bool?

      def archived=: (bool) -> bool

      attr_accessor brand_id: String?

      attr_accessor page_number: Integer?

      attr_accessor page_size: Integer?

      attr_accessor recurring: bool?

      def initialize: (
        ?archived: bool,
        ?brand_id: String?,
        ?page_number: Integer?,
        ?page_size: Integer?,
        ?recurring: bool?,
        ?request_options: Dodopayments::request_opts
      ) -> void

      def to_hash: -> {
        archived: bool,
        brand_id: String?,
        page_number: Integer?,
        page_size: Integer?,
        recurring: bool?,
        request_options: Dodopayments::RequestOptions
      }
    end
  end
end
