    type payment =
      {
        billing: Dodopayments::Models::BillingAddress,
        business_id: String,
        created_at: Time,
        currency: Dodopayments::Models::currency,
        customer: Dodopayments::Models::CustomerLimitedDetails,
        disputes: ::Array[Dodopayments::Models::Dispute],
