      attr_reader page_size: Integer?

      def page_size=: (Integer) -> Integer

      def initialize: (
        ?email: String,
        ?page_number: Integer,
        ?page_size: Integer,
        ?request_options: Dodopayments::request_opts
      ) -> void

      def to_hash: -> {
